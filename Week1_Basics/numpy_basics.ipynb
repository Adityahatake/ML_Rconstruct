    "import numpy as np\n",
    " \n",
    "a=np.array([[1,2,3],[4,5,6]])\n",
    "b=np.array([[1,1,1],[2,2,2]])\n",
    "\n",
    "print(a)\n",
    "print(b)\n",
    "\n",
    "print(\"Addition:\\n\" ,a+b)\n",
    "print(\"Dot product:\\n\" , np.dot(a ,b.T))\n",
    "print(\"mean of a:\\n\" , np.mean(a))\n",
    "print(\"mean of b:\\n\" , np.mean(b))\n",
    "\n",
    "\n"
   ]
